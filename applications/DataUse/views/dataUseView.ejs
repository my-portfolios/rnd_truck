

<!-- view -->
	<div class="download_bg">

		<% 
			let versionName = '';
			fileList.sort((c1, c2) => {
				if(c1.updateDt < c2.updateDt) { return -1; } 
				else if(c1.updateDt > c2.updateDt) { return 1; }
				return 0;
			});
			fileList.forEach(function(element) { %>
			<% if(element.fileDesc == 'thumbnail') { %>
				<p class="area"><img src="/file/image/view/<%- element.fileKey%>" class="download_img"></p>
			<% } else if(element.fileDesc == 'program') { versionName = element.remark; } %>
		<% }); %>
		
		<div class="download_bg1">
		<h1><%- dataUseInfo.datauseSj%></h1>
		<h4><%- dataUseInfo.datauseCn%></h4>
		<h5><%- dataUseInfo.datauseDev%> / <%- dataUseInfo.updateDt%></h5>
		<button type="button" id="btn_down" class="btn btn-danger" onclick="location.href='/data/download/<%- dataUseInfo.datauseKey%>/latest'">최신버전(<%- versionName %>) 다운로드</button>
		</div>	
								
	</div>

	<p class="text-center">
		<button type="button" id="btn_list" class="btn btn-primary" onclick="location.href='/data'">목록</button>
	</p>
<!-- view -->